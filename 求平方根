class Solution {
public:
    /**
     *
     * @param x int整型
     * @return int整型
     */
    int sqrt(int x) {
        // write code here
        if(x==1) return 1;
        int left = 1,right = x; //左右边界
        while(right>=left){
            int mid = (right+left)/2;  //中间值
            if(mid<=x/mid // mid*mid可能溢出，所以用除法
            if(mid>x/mid) right = mid -1;
            else  left = mid +1;
        }
        return 0;
    }
};